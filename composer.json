{
    "name": "platform/admin-default",
    "type": "platform-theme",
    "description": "Default admin theme.",
    "keywords": [
        "cartalyst",
        "platform",
        "laravel",
        "theme"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "cartalyst/composer-installers": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "theme",
        "branch-alias": {
            "dev-master": "10.0.x-dev"
        }
    }
}
